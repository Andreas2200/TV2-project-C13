   <left>
      <AnchorPane prefHeight="824.0" prefWidth="248.0" style="-fx-background-color: #d21e1e;" BorderPane.alignment="CENTER">
         <children>
            <VBox alignment="TOP_CENTER" prefHeight="824.0" prefWidth="265.0" spacing="10.0">
               <children>
                  <ImageView fx:id="userImageView" fitHeight="199.0" fitWidth="206.0" nodeOrientation="INHERIT" pickOnBounds="true" preserveRatio="true" />
