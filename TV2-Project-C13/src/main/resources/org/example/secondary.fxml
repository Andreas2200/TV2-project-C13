                                  <TableColumn prefWidth="131.0" text="Beskæftigelse" />
                                    <TableColumn prefWidth="174.0" text="Kontakt Information:" />
                                </columns>
                              </TableView>
                            </children>
                            <padding>
                                <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
                            </padding>
                        </VBox>
                     </children>
                  </AnchorPane>
               </children>
            </VBox>
         </children>
      </AnchorPane>
